<html t:type="Layout" t:title="${message:helReport-label}" t:id="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">
	<div class="row main-contents">
		<div class="col-md-12">
			<div class="card-box">
				<div class="title">
					<h4 class="text-dark header-title m-t-0" style="float:left">${message:jobListTitle}</h4>
				</div>
				<div class="row">
					<div class="col-sm-12">
						<table t:type="grid" t:id="grid"
							class="table table-striped table-bordered dataTable no-footer table-actions-bar"
							t:source="list" t:row="row" exclude="ordinal,val" t:add="allJobs">
							<p:AllJobsCell>
								${allJobs}
							</p:AllJobsCell>
						</table>
					</div>
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>

</html>