<html t:type="layout" title=""
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">
	<head>
		<script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"></script>
		<script type="text/javascript" src="http://d3js.org/d3.v3.min.js"></script>
	</head>
	<body>

		<div class="row top-widgets">
			<div class="col-lg-3">
				<div class="card-box blue-widget">
					<div class="bar-widget">
						<div class="table-box">
							<div class="table-detail" style="width:100px;">
								<span data-plugin="peity-bar" data-colors="#fff,#fff"
									data-width="82" data-height="45" style="display: none;">5,3,9,6,5,9,7,3,5,2,9,7</span>
								<svg class="peity" height="45" width="82">
									<rect fill="#fff" x="0" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="7" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="14" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="21" y="15" width="5" height="30"></rect>
									<rect fill="#fff" x="28" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="35" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="42" y="10" width="5" height="35"></rect>
									<rect fill="#fff" x="49" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="56" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="63" y="35" width="5" height="10"></rect>
									<rect fill="#fff" x="70" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="77" y="10" width="5" height="35"></rect>
								</svg>
							</div>

							<div class="table-detail">
								<h4 class="top-widget-number">${allJobCount}</h4>
							</div>
						</div>
						<div class="top-widget-title">
							${message:allJob}
						</div>

					</div>
				</div>
			</div>
			<div class="col-lg-3">
				<div class="card-box lightblue-widget">
					<div class="bar-widget">
						<div class="table-box">
							<div class="table-detail" style="width:100px;">
								<span data-plugin="peity-bar" data-colors="#fff,#fff"
									data-width="82" data-height="45" style="display: none;">5,3,9,6,5,9,7,3,5,2,9,7</span>
								<svg class="peity" height="45" width="82">
									<rect fill="#fff" x="0" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="7" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="14" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="21" y="15" width="5" height="30"></rect>
									<rect fill="#fff" x="28" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="35" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="42" y="10" width="5" height="35"></rect>
									<rect fill="#fff" x="49" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="56" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="63" y="35" width="5" height="10"></rect>
									<rect fill="#fff" x="70" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="77" y="10" width="5" height="35"></rect>
								</svg>
							</div>

							<div class="table-detail">
								<h4 class="top-widget-number">${newJobCount}</h4>
							</div>
						</div>
						<div class="top-widget-title">
							${message:newJobs}
						</div>

					</div>
				</div>
			</div>
			<div class="col-lg-3">
				<div class="card-box red-widget">
					<div class="bar-widget">
						<div class="table-box">
							<div class="table-detail" style="width:100px;">
								<span data-plugin="peity-bar" data-colors="#fff,#fff"
									data-width="82" data-height="45" style="display: none;">5,3,9,6,5,9,7,3,5,2,9,7</span>
								<svg class="peity" height="45" width="82">
									<rect fill="#fff" x="0" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="7" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="14" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="21" y="15" width="5" height="30"></rect>
									<rect fill="#fff" x="28" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="35" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="42" y="10" width="5" height="35"></rect>
									<rect fill="#fff" x="49" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="56" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="63" y="35" width="5" height="10"></rect>
									<rect fill="#fff" x="70" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="77" y="10" width="5" height="35"></rect>
								</svg>
							</div>

							<div class="table-detail">
								<h4 class="top-widget-number">${hasagdsanJobs}</h4>
							</div>
						</div>
						<div class="top-widget-title">
							${message:hasagdsanJob}
						</div>

					</div>
				</div>
			</div>
			<div class="col-lg-3">
				<div class="card-box green-widget">
					<div class="bar-widget">
						<div class="table-box">
							<div class="table-detail" style="width:100px;">
								<span data-plugin="peity-bar" data-colors="#fff,#fff"
									data-width="82" data-height="45" style="display: none;">5,3,9,6,5,9,7,3,5,2,9,7</span>
								<svg class="peity" height="45" width="82">
									<rect fill="#fff" x="0" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="7" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="14" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="21" y="15" width="5" height="30"></rect>
									<rect fill="#fff" x="28" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="35" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="42" y="10" width="5" height="35"></rect>
									<rect fill="#fff" x="49" y="30" width="5" height="15"></rect>
									<rect fill="#fff" x="56" y="20" width="5" height="25"></rect>
									<rect fill="#fff" x="63" y="35" width="5" height="10"></rect>
									<rect fill="#fff" x="70" y="0" width="5" height="45"></rect>
									<rect fill="#fff" x="77" y="10" width="5" height="35"></rect>
								</svg>
							</div>

							<div class="table-detail">
								<h4 class="top-widget-number">${allEmployees}</h4>
							</div>
						</div>
						<div class="top-widget-title">
							${message:allEmployees}
						</div>

					</div>
				</div>
			</div>
		</div>


		<div class="row main-contents">
			<div class="col-sm-12">
				<div class="card-box">

			<!-- 		<h4 class="text-dark header-title m-t-0">${message:barChartDashboard}</h4> -->

                   <!--  <t:charts id="mbars" t:chartData="barData" t:chartType="'bar'" />   -->
                    
                   <div id="jobchart" style="min-width: 310px; margin: 0 auto"></div>                    
                       
         
				</div>
			</div>
		</div>


		<div class="row main-contents">
			<div class="col-sm-6">
				<div class="card-box">


					<div class="text-left">
						 <div id="container" style="min-width: 310px; margin: 0 auto"></div> 
					</div>

				</div>
			</div>
			<div class="col-sm-6">
				<div class="card-box">
					<h4 class="text-dark header-title m-t-0">${message:accessLog}</h4>

					<div class="nicescroll p-20">
						<div class="timeline-2">
							<t:Grid
								class="table table-striped table-bordered dataTable no-footer table-actions-bar"
								t:source="aList" t:row="aRow"
								t:exclude="uuid,modifiedDate,createdDate,modifiedDateFormated,createdDateFormated,logoutDate"
								t:add="rowIndex,fullname" t:reorder="rowIndex,accessDate,fullname,ipAddress">
								<p:empty>
									<p>${message:nodata}</p>
								</p:empty>
								<p:rowIndexCell>
									${rowIndex}
								</p:rowIndexCell>
									<p:fullnameCell>
									${username}
								</p:fullnameCell>
								<p:accessDateCell>
									<t:output t:value="aRow?.accessDate" t:format="timeFormat" />
								</p:accessDateCell>
							</t:Grid>
							<a t:type="pagelink" t:page="AccessLogPage" title="">
								<!-- <i class="fa fa-user" style="padding-top: 8px;padding-left: 
									5px; padding-right: 4px;" /> -->
								<span style="float:right;">
									${message:more}
								</span>
							</a>

						</div>
					</div>
				</div>
			</div>
		</div>
		<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/data.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>
<table hidden='true' id="datatable">
    <thead>
        <tr>
            <th></th>
            <th>${message:total}</th>
            <th>${message:female}</th>
            <th>${message:berhsheeltei}</th>
        </tr>
    </thead>
    <tbody>
    <t:if test="${jan}">
        <tr>
            <th>Jan</th>
            <td>${totalJan}</td>
            <td>${totalFemaleJan}</td>
            <td>${totalImpJan}</td>
        </tr>
        </t:if>
        <t:if test="${feb}">
        <tr>
            <th>Feb</th>
           <td>${totalFeb}</td>
            <td>${totalFemaleFeb}</td>
            <td>${totalImpFeb}</td>
        </tr>
        </t:if>
        <t:if test="${mar}">
        <tr>
            <th>Mar</th>
            <td>${totalMar}</td>
            <td>${totalFemaleMar}</td>
            <td>${totalImpMar}</td>
        </tr>
        </t:if>
        <t:if test="${apr}">
        <tr>
            <th>Apr</th>
            <td>${totalApr}</td>
            <td>${totalFemaleApr}</td>
            <td>${totalImpApr}</td>
        </tr>
        </t:if>
        <t:if test="${may}">
        <tr>
            <th>May</th>
            <td>${totalMay}</td>
            <td>${totalFemaleMay}</td>
            <td>${totalImpMay}</td>
        </tr>
        </t:if>
        <t:if test="${jun}">
        <tr>
            <th>Jun</th>
            <td>${totalJun}</td>
            <td>${totalFemaleJun}</td>
            <td>${totalImpJun}</td>
        </tr>
        </t:if>
        <t:if test="${jul}">
        <tr>
            <th>Jul</th>
            <td>${totalJul}</td>
            <td>${totalFemaleJul}</td>
            <td>${totalImpJul}</td>
        </tr>
        </t:if>
        <t:if test="${aug}">
        <tr>
            <th>Aug</th>
           <td>${totalAug}</td>
            <td>${totalFemaleAug}</td>
            <td>${totalImpAug}</td>
        </tr>
        </t:if>
        <t:if test="${sep}">
        <tr>
            <th>Sep</th>
           <td>${totalSep}</td>
            <td>${totalFemaleSep}</td>
            <td>${totalImpSep}</td>
        </tr>
        </t:if>
        <t:if test="${oct}">
         <tr>
            <th>Oct</th>
           <td>${totalOct}</td>
            <td>${totalFemaleOct}</td>
            <td>${totalImpOct}</td>
        </tr>
        </t:if>
        <t:if test="${nov}">
        <tr>
            <th>Nov</th>
            <td>${totalNov}</td>
            <td>${totalFemaleNov}</td>
            <td>${totalImpNov}</td>
        </tr>
        </t:if>
        <t:if test="${dec}">
        <tr>
            <th>Dec</th>
           <td>${totalDec}</td>
            <td>${totalFemaleDec}</td>
            <td>${totalImpDec}</td>
        </tr>
        </t:if>
    </tbody>
</table>
<script>
$(function () {
    $('#container').highcharts({
        data: {
            table: 'datatable'
        },
        chart: {
            type: 'column'
        },
        title: {
            text: '${message:haritsaa}'
        },
        yAxis: {
            allowDecimals: false,
            title: {
                text: ''
            }
        },
        tooltip: {
            formatter: function () {
                return '<b>' + this.series.name + '</b><br/>' +
                    this.point.y + ' ' + this.point.name.toLowerCase();
            }
        }
    });
});

$(function () {
    $('#jobchart').highcharts({
        chart: {
            type: 'area'
        },
        title: {
            text: '${message:barChartDashboard}'
        },
        subtitle: {
            text: ''
        },
        xAxis: {
            categories: ${test},
            tickmarkPlacement: 'on',
            title: {
                enabled: false
            }
        },
        yAxis: {
            title: {
                text: ''
            },
            labels: {
                formatter: function () {
                    return this.value / 1000;
                }
            }
        },
        tooltip: {
            shared: true,
            valueSuffix: ' '
        },
        plotOptions: {
            area: {
                stacking: 'normal',
                lineColor: '#666666',
                lineWidth: 1,
                marker: {
                    lineWidth: 1,
                    lineColor: '#666666'
                }
            }
        },
        series: [{
            name: '${message:ajliinBairniiToo}',
            data: [5, 6, 9, 7, 10, 6, 2]
        }, {
            name: '${message:newJobs}',
            data: [2, 3, 1, 6, 13, 0, 6]
        }]
    });
});

</script>
	</body>
</html>
