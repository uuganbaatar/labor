<t:container
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
	<t:if test="pie">
		<div id="pie"
			style="min-width: 310px; height: 400px; max-width: 600px; margin: 0 auto"></div>
	</t:if>

	<t:if test="bar">
		<div id="bar"
			style="min-width: 310px; height: 400px; max-width: 600px; margin: 0 auto;color:red;"></div>
	</t:if>

	
</t:container>